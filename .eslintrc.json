{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "chrome": "readonly"
  },
  "rules": {
    "no-console": "off",
    "no-use-before-define": ["error", { "functions": false, "classes": true, "variables": true }],
    "import/extensions": ["error", "always"]
  }
}
